  "scripts": {
    "start": "node \"node_modules/.bin/babel-node\" bin/www",
    "start-api": "nodemon --exec \"node_modules/.bin/nodejs-dashboard\" \"node_modules/.bin/babel-node\" bin/www",
    "open:src": "node_modules/.bin/babel-node tools/srcServer.js",
    "react-test": "jest src/js/__test__/*.spec.js",
    "react-test:watch": "npm run react-test -- --watch",
    "api-test": "jest app/__test__/*.spec.js",
    "api-test:watch": "npm run api-test -- --watch",
    "lint": "node_modules/.bin/esw webpack.config.* src tools app",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm-run-all --parallel api-test react-test",
    "test:watch": "npm-run-all --parallel api-test:watch react-test:watch",
    "build": "node_modules/.bin/babel-node tools/build",
    "build-html": "node_modules/.bin/babel-node tools/buildHTML"
  },
